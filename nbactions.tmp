<?xml version="1.0" encoding="UTF-8"?>
<actions>
        <action>
            <actionName>run</actionName>
            <packagings>
                <packaging>jar</packaging>
            </packagings>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:3.0.0:exec</goal>
            </goals>
            <properties>
                <exec.vmArgs>-Dspring.profiles.active=dev</exec.vmArgs>
                <exec.args>-Dspring.profiles.active=dev -classpath %classpath com.FincaReservas.Reto3Finca.Reto3FincaApplication</exec.args>
                <exec.appArgs></exec.appArgs>
                <exec.mainClass>com.FincaReservas.Reto3Finca.Reto3FincaApplication</exec.mainClass>
                <exec.executable>java</exec.executable>
            </properties>
        </action>
        <action>
            <actionName>debug</actionName>
            <packagings>
                <packaging>jar</packaging>
            </packagings>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:3.0.0:exec</goal>
            </goals>
            <properties>
                <exec.vmArgs>-Dspring.profiles.active=dev -agentlib:jdwp=transport=dt_socket,server=n,address=${jpda.address}</exec.vmArgs>
                <exec.args>-agentlib:jdwp=transport=dt_socket,server=n,address=${jpda.address} -Dspring.profiles.active=dev -classpath %classpath com.FincaReservas.Reto3Finca.Reto3FincaApplication</exec.args>
                <exec.appArgs></exec.appArgs>
                <exec.mainClass>com.FincaReservas.Reto3Finca.Reto3FincaApplication</exec.mainClass>
                <exec.executable>java</exec.executable>
                <jpda.listen>true</jpda.listen>
            </properties>
        </action>
        <action>
            <actionName>profile</actionName>
            <packagings>
                <packaging>jar</packaging>
            </packagings>
