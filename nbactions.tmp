                <exec.args>-Dspring.profiles.active=dev -classpath %classpath com.FincaReservas.Reto3Finca.Reto3FincaApplication</exec.args>
                <exec.mainClass>com.FincaReservas.Reto3Finca.Reto3FincaApplication</exec.mainClass>
                <exec.executable>java</exec.executable>
                <exec.appArgs></exec.appArgs>
